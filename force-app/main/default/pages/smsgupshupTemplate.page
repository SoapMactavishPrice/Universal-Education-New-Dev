<apex:page id="smsgupshupTemplate" standardController="smsgupshupTemplate__c" extensions="smsgupshupTemplateController">
    <apex:sectionHeader title="New Template" />
    <apex:form >
        <apex:pageBlock title="Templates" >
            <apex:pageBlockButtons >
                <apex:commandButton value="Save" action="{!Save}"/>
            </apex:pageBlockButtons>
            <apex:pageBlockSection showHeader="true"  columns="1"  >
                <apex:inputField required="true" value="{!smsgupshupTemplate__c.Name__c}"/>
                <apex:inputField required="true" value="{!smsgupshupTemplate__c.Description__c}" style="width:550px;height:250px"/>
            </apex:pageBlockSection>
        </apex:pageBlock>
    </apex:form>
</apex:page>