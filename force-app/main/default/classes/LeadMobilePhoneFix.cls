public class LeadMobilePhoneFix implements Database.Batchable<SObject> {
    
    public Database.QueryLocator start(Database.BatchableContext bc) {
        // Query all leads with incorrect mobile phone format
        return Database.getQueryLocator([
            SELECT Id, MobilePhone 
            FROM Lead 
            WHERE MobilePhone LIKE '+91________'
            AND Acadmic_Year__c = '2026-2027'
        ]);
    }
    // 
    public void execute(Database.BatchableContext bc, List<Lead> scope) {
        List<Lead> leadsToUpdate = new List<Lead>();
        
        for(Lead l : scope) {
            if(l.MobilePhone != null && l.MobilePhone.startsWith('+91')) {
                // Insert '91' after '+91'
                String updatedPhone = '+91' + '91' + l.MobilePhone.substring(3);
                l.MobilePhone = updatedPhone;
                leadsToUpdate.add(l);
            }
        }
        
        if(!leadsToUpdate.isEmpty()) {
            Database.SaveResult[] results = Database.update(leadsToUpdate, false);
            
            // Log any errors
            for(Database.SaveResult sr : results) {
                if(!sr.isSuccess()) {
                    for(Database.Error err : sr.getErrors()) {
                        System.debug('ERROR on Lead ' + sr.getId() + ': ' + err.getMessage());
                    }
                }
            }
        }
    }
    
    public void finish(Database.BatchableContext bc) {
        System.debug('LeadMobilePhoneFix batch completed');
        
        // Optional: Query to verify remaining records
        Integer remaining = [SELECT COUNT() FROM Lead WHERE MobilePhone LIKE '+91________'];
        System.debug('Remaining records to fix: ' + remaining);
    }

